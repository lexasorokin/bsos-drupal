{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.9",
        "drupal/admin_toolbar": "^2.4",
        "drupal/administerusersbyrole": "^3.3",
        "drupal/allowed_formats": "^2.0",
        "drupal/anchor_link": "^1.9",
        "drupal/asset_injector": "^2.12",
        "drupal/auto_entitylabel": "^3.0@beta",
        "drupal/auto_height": "2.0.x-dev@dev",
        "drupal/background_image_formatter": "^1.7",
        "drupal/backup_migrate": "^5.0",
        "drupal/bg_image_formatter": "^1.16",
        "drupal/block_class": "^1.3",
        "drupal/block_content_permissions": "^1.10",
        "drupal/broken_link": "^3.1",
        "drupal/captcha": "^1.8",
        "drupal/cas": "^1.7",
        "drupal/checklistapi": "^1.11",
        "drupal/ckeditor_bootstrap_buttons": "^2.0",
        "drupal/ckeditor_tableresize": "^1.1",
        "drupal/clientside_validation": "^4.0",
        "drupal/computed_field": "^3.0@alpha",
        "drupal/config_rewrite": "^1.5",
        "drupal/config_update": "^1.7",
        "drupal/content_sync": "^3.0@beta",
        "drupal/context": "^4.1",
        "drupal/core-composer-scaffold": "^9.4",
        "drupal/core-project-message": "^9.4",
        "drupal/core-recommended": "^9.4",
        "drupal/crop": "^2.3",
        "drupal/cshs": "^3.6",
        "drupal/css_editor": "^2.0",
        "drupal/csv_serialization": "^1.5",
        "drupal/ctools": "^3.13",
        "drupal/custom_breadcrumbs": "^1.0",
        "drupal/devel": "^5.0",
        "drupal/devel_entity_updates": "^4.0",
        "drupal/diff": "^1.1",
        "drupal/ds": "^3.15",
        "drupal/easy_breadcrumb": "^2.0",
        "drupal/editor_advanced_link": "^2.0",
        "drupal/entity": "^1.4",
        "drupal/entity_browser": "^2.8",
        "drupal/entity_class_formatter": "^1.3",
        "drupal/entity_limit": "^2.0@alpha",
        "drupal/entity_reference_revisions": "^1.10",
        "drupal/eva": "^3.0",
        "drupal/externalauth": "^1.4",
        "drupal/extlink": "^1.7",
        "drupal/features": "^3.12",
        "drupal/feeds": "^3.0@beta",
        "drupal/fences": "^3.0",
        "drupal/field_group": "^3.4",
        "drupal/field_permissions": "^1.2",
        "drupal/file_delete": "^2.0",
        "drupal/file_mdm": "^2.5",
        "drupal/fontawesome": "^2.24",
        "drupal/fullcalendar": "^1.0@alpha",
        "drupal/google_analytics": "^4.0",
        "drupal/google_tag": "^1.5",
        "drupal/honeypot": "^2.1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
