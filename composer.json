        "drupal/file_mdm": "^2.5",
        "drupal/fontawesome": "^2.24",
        "drupal/fpa": "^3.0",
        "drupal/fullcalendar": "^1.0@alpha",
        "drupal/google_analytics": "^4.0",
        "drupal/google_tag": "^1.5",
        "drupal/honeypot": "^2.1",
